package mayar;

/**
 *
 * @author user
 */
public class shippable_products extends Products implements ShippingService {
    float weight;
    public shippable_products(String name, float price, int quantity, boolean shippable, boolean ex) {
        super(name, price, quantity, shippable, ex);
        this.weight=weight ;
        
    }
    @Override
    public float getweight(){
        return weight;
        
    }
    
}
